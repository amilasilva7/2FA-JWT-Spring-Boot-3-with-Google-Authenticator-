				{
					"name": "Login With TOTP",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.environment.set(\"accessToken\", pm.response.json().accessToken);",
									"pm.environment.set(\"refreshToken\", pm.response.json().refreshToken);"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"osapsilva@gmail.com\",\n    \"password\": \"DwsS!3200\",\n    \"code\": \"342517\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": "localhost:8080/api/v1/auth/authenticate"
					},
					"response": []
				},
				{
					"name": "Change Password",
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": {
								"token": "{{refreshToken}}"
							}
						},
						"method": "PATCH",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"currentPassword\": \"DwsS!3200\",\n    \"newPassword\": \"DwsS!32001\",\n    \"confirmationPassword\": \"DwsS!32001\"\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": "localhost:8080/api/v1/users"
					},
					"response": []
				},
